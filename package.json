{
  "name": "remark-preset-bpmn-io",
  "version": "0.3.1",
  "description": "A remark preset for bpmn.io projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/remark-preset-bpmn-io"
  },
  "author": "Nico Rehwaldt <https://github.com/nikku>",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
